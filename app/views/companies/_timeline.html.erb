<section class="timeline">
    <% @timeline_items.each do |item|%>


        <div class='timeline-card'>
            <div class='timeline-card-img centered'><%= image_tag(item.image.url, class: 'stanford-logo') %></div>
            <div class='tmln-card-title centered'><%= item.content%></div>
            <div class='timeline-date'><%= item.created_date %></div>
        </div>

    <% end %>

</section>
<div class='timeline-line'></div>
